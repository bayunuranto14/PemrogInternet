<html>
    <head>
        <script type="text/javascript" src="jquery-1.7.1.js"></script>
        <script type="text/javasciprt">
        $ (document) .ready(function() {
            $ (".intro") .css ("background-color","orange");

            $ ("#title") .click(function(){
                $ ("#title") .css ("background-color","yellow"); 
            });

            $ ("input") .focus(function(){
                $ ("input") .css ("background-color","#FFFFCC"); 
            });

            $ ("input") .blur(function(){
                $ ("input") .css ("background-color","D6D6FF"); 
            });

            $ (".intro") .mouseover(function(){
                $ (".intro") .css ("background-color","D6D6FF"); 
            });

            $ (".intro") .click(function(){
                $ (".intro") .css ("background-color","orange"); 
            });
        });
    </script>
    </head>
    <body>
        <h2 id="title">Welcome to My Page</h2>
        <p class="intro">jQuery Scripting</p>
        <p class="intro">jQuery Example</p>
        <input type="text"/>
    </body>
</html>